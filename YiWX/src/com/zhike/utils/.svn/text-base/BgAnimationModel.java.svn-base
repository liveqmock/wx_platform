package com.zhike.utils;

public class BgAnimationModel {
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getValue() {
		return value;
	}

	public void setValue(String value) {
		this.value = value;
	}

	//动画名称
	public String name;
	//图片地址
	public String value;
	
	public BgAnimationModel(String name,String value){
		this.name = name;
		this.value = value;
	}
}
