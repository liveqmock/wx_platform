	<script>
	$(function(){
		$("#focus").slideDown(800);
		$('#caseLogo').mixitup({
			showOnLoad: 'ctypep',
			effects: ['fade','rotateY'],
			onMixStart: function(){
				$("#cnt").fadeOut();
			},
			onMixLoad: function(){
				$(".mix_all").filter(":visible").first().find("a").addClass("on");
				$(".caseCnt").load($(".mix_all").filter(":visible").first().find("a").attr("href")+" #cnt", function(){
					$("#cnt").fadeIn();
				});
			},
			onMixEnd: function(){
				$(".mix a").removeClass("on");
				$(".mix_all").filter(":visible").first().find("a").addClass("on");
				$(".caseCnt").load($(".mix_all").filter(":visible").first().find("a").attr("href")+" #cnt", function(){
					$("#cnt").fadeIn();
				});
			}
		});
		$(".mix a").click(function(){
			return false;
		});
		$(".mix a").mouseenter(function () {
			$(".mix a").removeClass("on");
			$(this).addClass("on");
			$(".caseCnt").load($(this).attr("href")+" #cnt", function(){
				$("#cnt").fadeIn();
			});
		});
	});
	</script>
