* {
	PADDING-BOTTOM: 0px; MARGIN: 0px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; PADDING-TOP: 0px
}
A {
	OUTLINE-STYLE: none; OUTLINE-COLOR: invert; OUTLINE-WIDTH: medium; COLOR: #555; TEXT-DECORATION: none
}
A:hover {
	COLOR: #3fa9f5
}
A {
	
}
UL {
	LIST-STYLE-TYPE: none; LIST-STYLE-IMAGE: none
}
H4 {
	FONT-WEIGHT: normal
}
H3 {
	FONT-WEIGHT: normal
}
H2 {
	FONT-WEIGHT: normal
}
H1 {
	FONT-WEIGHT: normal
}
IMG {
	BORDER-BOTTOM: medium none; BORDER-LEFT: medium none; BORDER-TOP: medium none; BORDER-RIGHT: medium none
}
HTML {
	MIN-WIDTH: 1000px; FONT: 14px/24px "풕턼환쥌 Light", "풕턼환쥌", arial, sans-serif; BACKGROUND: #f2f2f2; COLOR: #666
}
BODY {
	MIN-WIDTH: 1000px; FONT: 14px/24px "풕턼환쥌 Light", "풕턼환쥌", arial, sans-serif; BACKGROUND: #f2f2f2; COLOR: #666
}
.l {
	FLOAT: left
}
.r {
	FLOAT: right
}
.g {
	MARGIN: 0px auto; WIDTH: 1000px
}
.green {
	COLOR: #48b732
}
.fs24 {
	FONT-SIZE: 24px
}
.hc {
	TEXT-ALIGN: center
}
#header {
	BORDER-BOTTOM: #eee 2px solid; BACKGROUND: #fff; HEIGHT: 100px
}
#header .g {
	HEIGHT: 100px
}
#logo A {
	WIDTH: 290px; DISPLAY: block; BACKGROUND: url(./logo.png) no-repeat; HEIGHT: 100px
}
#area {
	WIDTH: 110px; HEIGHT: 100px
}
#areaBtn {
	TEXT-ALIGN: center; WIDTH: 110px; DISPLAY: block; BACKGROUND: url(./areaArrow.png) no-repeat right 0px; COLOR: #b3b3b3; FONT-SIZE: 22px; CURSOR: pointer; PADDING-TOP: 42px
}
#areaBtn:hover {
	BACKGROUND-POSITION: right -100px; COLOR: #3fa9f5
}
#nav {
	PADDING-BOTTOM: 0px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; HEIGHT: 40px; MARGIN-LEFT: 10px; PADDING-TOP: 40px
}
#nav LI {
	FLOAT: left; HEIGHT: 40px; FONT-SIZE: 18px
}
#nav LI A {
	PADDING-BOTTOM: 0px; PADDING-LEFT: 30px; PADDING-RIGHT: 30px; PADDING-TOP: 0px
}
#nav LI A.on {
	COLOR: #55d13f
}
#loginBox {
	WIDTH: 142px; HEIGHT: 36px; PADDING-TOP: 35px
}
#loginBox A {
	TEXT-INDENT: -9999px; WIDTH: 70px; DISPLAY: inline-block; BACKGROUND: url(./loginBox_bg.png) no-repeat; FLOAT: left; HEIGHT: 36px; MARGIN-RIGHT: 1px
}
#loginBox A.loginBtn:hover {
	BACKGROUND-POSITION: 0px -36px
}
#loginBox A.regBtn {
	BACKGROUND-POSITION: right 0px
}
#loginBox A.regBtn:hover {
	BACKGROUND-POSITION: right -36px
}
#crumbs {
	PADDING-BOTTOM: 3px; PADDING-LEFT: 0px; WIDTH: 990px; PADDING-RIGHT: 0px; FONT-SIZE: 12px; PADDING-TOP: 3px
}
#catfocus {
	POSITION: relative; DISPLAY: none; HEIGHT: 160px; OVERFLOW: hidden
}
.catfocus {
	POSITION: absolute; WIDTH: 2000em; HEIGHT: 160px
}
.catfocus LI {
	WIDTH: 1000px; FLOAT: left; HEIGHT: 160px
}
.filterBox {
	POSITION: relative; PADDING-BOTTOM: 2px; LINE-HEIGHT: 40px; MARGIN-TOP: 15px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; BACKGROUND: url(./filter_bg.png) no-repeat; HEIGHT: 40px; PADDING-TOP: 2px
}
.filterBox H1 {
	POSITION: absolute; COLOR: #bbb; FONT-SIZE: 17px; TOP: 2px; FONT-WEIGHT: normal; LEFT: 15px
}
.filterBtn {
	MARGIN: 0px auto; WIDTH: 280px; HEIGHT: 40px
}
.filterBtn LI {
	TEXT-ALIGN: center; WIDTH: 70px; FLOAT: left; CURSOR: pointer
}
.filterBtn LI.active {
	BACKGROUND: url(./filterBtn_bg.png) no-repeat 0px 0px; COLOR: #fff
}
.listType {
	POSITION: absolute; TEXT-ALIGN: right; WIDTH: 80px; HEIGHT: 40px; TOP: 2px; RIGHT: 15px
}
.listType A {
	COLOR: #3fa9f5
}
#footer {
	LINE-HEIGHT: 28px; BACKGROUND: #fff; COLOR: #666; BORDER-TOP: #eee 2px solid
}
#footer .g {
	OVERFLOW: auto
}
.fnavBox {
	PADDING-TOP: 30px
}
.fnav .liblock {
	WIDTH: 160px; FLOAT: left
}
.fnav .liblock4 {
	WIDTH: 180px
}
.fnav H3 {
	LINE-HEIGHT: 32px; COLOR: #333; FONT-SIZE: 18px
}
.fnav H3 A {
	LINE-HEIGHT: 32px; COLOR: #333; FONT-SIZE: 18px
}
.cpr {
	PADDING-BOTTOM: 10px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; PADDING-TOP: 10px
}
.cpr A {
	MARGIN-RIGHT: 15px
}
.cooperative {
	MARGIN: 10px auto 0px; WIDTH: 1000px; BACKGROUND: url(./cooperative.png) no-repeat; HEIGHT: 60px
}
#overlay {
	BACKGROUND-IMAGE: url(./transparent.png); PADDING-BOTTOM: 20px; PADDING-LEFT: 20px; WIDTH: 800px; PADDING-RIGHT: 20px; DISPLAY: none; HEIGHT: 536px; OVERFLOW: hidden; PADDING-TOP: 20px
}
#vdoBox {
	BACKGROUND-IMAGE: url(./vdobg.png); WIDTH: 640px; DISPLAY: none; HEIGHT: 360px
}
DIV.overlayTool {
	HEIGHT: 60px
}
.oImgBox {
	HEIGHT: 60px; MARGIN-RIGHT: 10px
}
.oImgBox IMG {
	WIDTH: 60px; HEIGHT: 60px; border-radius: 30px
}
.oTitle H3 {
	PADDING-BOTTOM: 0px; LINE-HEIGHT: 28px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; FONT-SIZE: 26px; PADDING-TOP: 5px
}
.oTitle P {
	COLOR: #aaa
}
.oLink {
	PADDING-RIGHT: 35px
}
.oLink A {
	LINE-HEIGHT: 30px; PADDING-LEFT: 30px; DISPLAY: inline-block; BACKGROUND: url(./oLink.png) no-repeat; FLOAT: left; COLOR: #c3c4c0; MARGIN-RIGHT: 10px
}
.oLink A:hover {
	COLOR: #3fa9f5
}
.oLink A.allM:hover {
	BACKGROUND-POSITION: 0px -30px
}
.oLink A.newW {
	BACKGROUND-POSITION: 0px -60px
}
.oLink A.newW:hover {
	BACKGROUND-POSITION: 0px -90px
}
DIV.contentWrap {
	OVERFLOW-Y: auto; MARGIN-TOP: 15px; WIDTH: 800px; BACKGROUND: #f5f5f5; HEIGHT: 460px; OVERFLOW: hidden; border-radius: 5px
}
#mdCnt {
	HEIGHT: 460px; OVERFLOW: hidden
}
.close {
	BACKGROUND-IMAGE: url(./close.png); POSITION: absolute; WIDTH: 50px; HEIGHT: 50px; TOP: 10px; CURSOR: pointer; RIGHT: 10px
}
.close:hover {
	BACKGROUND-POSITION: 0px -50px
}
#cnt {
	PADDING-BOTTOM: 0px; PADDING-LEFT: 20px; PADDING-RIGHT: 0px; DISPLAY: none; OVERFLOW: auto; PADDING-TOP: 30px
}
.imgBox {
	DISPLAY: inline-block; FLOAT: left; MARGIN-RIGHT: 20px
}
.codeBox {
	PADDING-BOTTOM: 0px; PADDING-LEFT: 0px; WIDTH: 190px; PADDING-RIGHT: 0px; HEIGHT: 300px; PADDING-TOP: 90px
}
.codebg {
	TEXT-ALIGN: center; PADDING-BOTTOM: 6px; PADDING-LEFT: 6px; PADDING-RIGHT: 6px; BACKGROUND: url(./codebg.png) no-repeat center 50%; HEIGHT: 138px; PADDING-TOP: 6px
}
.codetext {
	TEXT-ALIGN: center; LINE-HEIGHT: 34px; FONT-SIZE: 16px; PADDING-TOP: 5px
}
.cname {
	COLOR: #000; FONT-WEIGHT: 800
}
#service {
	POSITION: fixed; FILTER: alpha(opacity=90); MARGIN-TOP: -120px; WIDTH: 36px; HEIGHT: 280px; TOP: 50%; RIGHT: 0px; _position: absolute; -moz-opacity: 0.9; -khtml-opacity: 0.9; opacity: 0.9
}
#service A {
	TEXT-INDENT: -9999px; WIDTH: 36px; DISPLAY: block; MARGIN-BOTTOM: 1px; BACKGROUND: url(./service.png) no-repeat; HEIGHT: 100px; CURSOR: pointer
}
#service A.mscBtn {
	BACKGROUND: url(./musicBtn.gif) #fff no-repeat; HEIGHT: 38px
}
#service A.goTop {
	WIDTH: 36px; BACKGROUND-POSITION: 0px bottom; HEIGHT: 38px
}
#service A.goTop:hover {
	BACKGROUND-POSITION: -36px bottom
}
#service A.pause {
	BACKGROUND-POSITION: 0px bottom
}
#service .srvLog {
	
}
#service .srvLog:hover {
	BACKGROUND-POSITION: -36px 0px
}
#service .srvCns {
	BACKGROUND-POSITION: 0px -100px
}
#service .srvCns:hover {
	BACKGROUND-POSITION: -36px -100px
}
#ologinBox {
	BACKGROUND-IMAGE: url(./loginbg.png); WIDTH: 410px; DISPLAY: none; HEIGHT: 360px
}
#ologinBox H3 {
	PADDING-BOTTOM: 0px; LINE-HEIGHT: 36px; PADDING-LEFT: 38px; PADDING-RIGHT: 0px; HEIGHT: 36px; COLOR: #7ac943; FONT-SIZE: 30px; PADDING-TOP: 25px
}
#ologinBox UL {
	PADDING-BOTTOM: 0px; PADDING-LEFT: 35px; PADDING-RIGHT: 35px; PADDING-TOP: 15px
}
#ologinBox LI {
	PADDING-LEFT: 50px; WIDTH: 290px; MARGIN-BOTTOM: 10px; BACKGROUND: url(./iptbg.png) no-repeat; HEIGHT: 48px
}
#ologinBox LI.l2 {
	BACKGROUND-POSITION: 0px -58px
}
#ologinBox LI.l3 {
	TEXT-ALIGN: right; BACKGROUND: none transparent scroll repeat 0% 0%; HEIGHT: 24px
}
#ologinBox LI.l4 {
	PADDING-BOTTOM: 0px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; BACKGROUND: none transparent scroll repeat 0% 0%; HEIGHT: 50px; PADDING-TOP: 0px
}
#ologinBox .ipt {
	BORDER-BOTTOM: medium none; BORDER-LEFT: medium none; PADDING-BOTTOM: 10px; OUTLINE-STYLE: none; OUTLINE-COLOR: invert; PADDING-LEFT: 0px; OUTLINE-WIDTH: medium; WIDTH: 285px; PADDING-RIGHT: 5px; FONT: 18px/28px "풕턼환쥌 Light", "풕턼환쥌", arial, sans-serif; BACKGROUND: none transparent scroll repeat 0% 0%; HEIGHT: 28px; BORDER-TOP: medium none; BORDER-RIGHT: medium none; PADDING-TOP: 10px
}
#btn-login {
	BORDER-BOTTOM: medium none; BORDER-LEFT: medium none; OUTLINE-STYLE: none; OUTLINE-COLOR: invert; OUTLINE-WIDTH: medium; WIDTH: 340px; BACKGROUND: url(./images/green/iptbg.png) no-repeat 0px -116px; HEIGHT: 50px; BORDER-TOP: medium none; CURSOR: pointer; BORDER-RIGHT: medium none
}
#btn-login:hover {
	BACKGROUND-POSITION: 0px bottom
}
.loginLink {
	PADDING-BOTTOM: 0px; LINE-HEIGHT: 24px; PADDING-LEFT: 35px; PADDING-RIGHT: 35px; HEIGHT: 24px; PADDING-TOP: 32px
}
.loginLink A.r {
	COLOR: #7ac943
}
