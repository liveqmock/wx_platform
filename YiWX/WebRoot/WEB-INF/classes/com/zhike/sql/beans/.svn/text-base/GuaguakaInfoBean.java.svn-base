package com.zhike.sql.beans;

public class GuaguakaInfoBean {
	
	public long id;
	public long service_id;//活动ID
	public String openid;//用户唯一标识
	public String sn;//兑奖SN码
	public String contact;//联系方式
	public String createtime;
	public String updatetime;
	public int type;//使用状态 ：-1,未下发,0未中奖，1已下发，2已兑奖
	public int todaytime;//今天次数
	public int totaltime;//总共次数
	public int prizetype;//中奖类型
	public int from;
	public int count;
	
	
	
	public int getFrom() {
		return from;
	}
	public void setFrom(int from) {
		this.from = from;
	}
	public int getCount() {
		return count;
	}
	public void setCount(int count) {
		this.count = count;
	}
	public String getSn() {
		return sn;
	}
	public void setSn(String sn) {
		this.sn = sn;
	}
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public long getService_id() {
		return service_id;
	}
	public void setService_id(long serviceId) {
		service_id = serviceId;
	}
	public String getOpenid() {
		return openid;
	}
	public void setOpenid(String openid) {
		this.openid = openid;
	}
	
	public String getContact() {
		return contact;
	}
	public void setContact(String contact) {
		this.contact = contact;
	}
	
	public String getCreatetime() {
		return createtime;
	}
	public void setCreatetime(String createtime) {
		this.createtime = createtime;
	}
	public String getUpdatetime() {
		return updatetime;
	}
	public void setUpdatetime(String updatetime) {
		this.updatetime = updatetime;
	}
	public int getType() {
		return type;
	}
	public void setType(int type) {
		this.type = type;
	}
	public int getTodaytime() {
		return todaytime;
	}
	public void setTodaytime(int todaytime) {
		this.todaytime = todaytime;
	}
	public int getTotaltime() {
		return totaltime;
	}
	public void setTotaltime(int totaltime) {
		this.totaltime = totaltime;
	}
	public int getPrizetype() {
		return prizetype;
	}
	public void setPrizetype(int prizetype) {
		this.prizetype = prizetype;
	}
	
	
}
