#grid {
	WIDTH: 1016px; BACKGROUND: url(./w1000bg.png) repeat-y center top; FONT-SIZE: 0px; PADDING-TOP: 15px; _width: 1032px
}
.grid {
	
}
.grid LI {
	Z-INDEX: 0; POSITION: relative; MARGIN: 0px 7px 10px 8px; WIDTH: 188px; HEIGHT: 200px; FONT-SIZE: 14px
}
.grid LI {
	
}
.grid LI.hover {
	BACKGROUND: url(./block_bg.png) no-repeat; CURSOR: pointer
}
.grid LI H3 {
	LINE-HEIGHT: 28px; HEIGHT: 140px; FONT-SIZE: 20px; FONT-WEIGHT: normal
}
.grid LI H3 A {
	Z-INDEX: 100; POSITION: absolute; TEXT-ALIGN: center; WIDTH: 188px; DISPLAY: block; HEIGHT: 185px; TOP: 0px; PADDING-TOP: 15px; LEFT: 0px
}
.grid LI H3 IMG {
	WIDTH: 86px; HEIGHT: 86px; border-radius: 43px
}
.grid LI H3 SPAN {
	DISPLAY: block; _padding-top: 7px
}
.grid LI P {
	Z-INDEX: 10; TEXT-ALIGN: center; LINE-HEIGHT: 22px; COLOR: #888; FONT-SIZE: 13px
}
#grid .mix {
	DISPLAY: none; opacity: 0
}
.prdTool {
	PADDING-BOTTOM: 25px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; MARGIN-BOTTOM: 25px; BACKGROUND: url(./prdTool.png) repeat-y; HEIGHT: 154px; BORDER-TOP: #e6e6e6 1px solid; PADDING-TOP: 25px
}
.prdTool A {
	TEXT-ALIGN: center; WIDTH: 300px; DISPLAY: inline-block; BACKGROUND: #9ad687; FLOAT: left; HEIGHT: 120px; COLOR: #fff; MARGIN-LEFT: 25px; FONT-SIZE: 16px; PADDING-TOP: 34px; border-radius: 3px; text-shadow: 0 1px 1px rgba(0, 0, 0, 0.15); box-shadow: 0 1px 2px rgba(0, 0, 0, 0.2)
}
.prdTool SPAN {
	LINE-HEIGHT: 36px; DISPLAY: block
}
.prdTool .ptTitle {
	LINE-HEIGHT: 52px; PADDING-LEFT: 60px; BACKGROUND: url(./prdTitle.png) no-repeat 50px 0px; FONT-SIZE: 35px; OVERFLOW: hidden
}
.prdTool .ptHelp {
	BACKGROUND: #87b3d4
}
.ptHelp .ptTitle {
	BACKGROUND-POSITION: 50px -52px
}
.prdTool .ptSrvs {
	BACKGROUND: #db5f68
}
.ptSrvs .ptTitle {
	PADDING-LEFT: 0px; BACKGROUND: none transparent scroll repeat 0% 0%
}
